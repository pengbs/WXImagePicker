<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.lzy.imagepickerdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name="com.rxxb.imagepickerdemo.GApp"
        android:theme="@style/AppTheme">
        <activity android:name="com.rxxb.imagepickerdemo.ImagePickerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.rxxb.imagepickerdemo.wxdemo.WxDemoActivity"/>

        <activity
            android:name="com.rxxb.imagepicker.ui.ImagePreviewDelActivity"
            android:theme="@style/ImagePickerThemeFullScreen"/>

    </application>

</manifest>
